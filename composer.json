{
    "name": "kaishiyoku/astolfo-twitter-poster",
    "description": "An astolfo picture for every day",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "abraham/twitteroauth": "1.1.0",
        "guzzlehttp/guzzle": "6.5.2",
        "laravel/lumen-framework": "7.0.0",
        "league/flysystem": "1.0.65",
        "vlucas/phpdotenv": "4.1.2",
        "woeler/phpdiscord": "2.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "2.6.7",
        "fzaninotto/faker": "1.9.1",
        "mockery/mockery": "1.3.1",
        "phpunit/phpunit": "8.5.2"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
