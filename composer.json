{
    "name": "kaishiyoku/astolfo-twitter-poster",
    "description": "An astolfo picture for every day",
    "keywords": ["astolfo"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "abraham/twitteroauth": "^2.0.1",
        "guzzlehttp/guzzle": "^7.3.0",
        "laravel/lumen-framework": "^8.2.3",
        "league/flysystem": "^2.1.0",
        "vlucas/phpdotenv": "^5.3.0",
        "woeler/phpdiscord": "^2.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10.0",
        "fzaninotto/faker": "^1.9.2",
        "mockery/mockery": "^1.4.3",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
