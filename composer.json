{
    "name": "kaishiyoku/astolfo-twitter-poster",
    "description": "An astolfo picture for every day",
    "keywords": ["astolfo"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "abraham/twitteroauth": "2.0.1",
        "guzzlehttp/guzzle": "7.2.0",
        "laravel/lumen-framework": "8.2.1",
        "league/flysystem": "1.1.3",
        "vlucas/phpdotenv": "5.2.0",
        "woeler/phpdiscord": "2.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "2.8.2",
        "fzaninotto/faker": "1.9.2",
        "mockery/mockery": "1.4.2",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
